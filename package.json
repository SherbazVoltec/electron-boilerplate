{
  "name": "voltec electron webpack skeleon",
  "version": "1.0.0",
  "description": "Voltec Pty Ltd.",
  "main": "electron_entry.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "webpack serve",
    "eldev": "webpack --mode production -o ./dist && electron-forge start --app-path dist/electron_entry.js",
